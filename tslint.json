{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-boolean-literal-compare": true,
    "variable-name": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "import-name": false,
    "jsx-boolean-value": false
  }
}